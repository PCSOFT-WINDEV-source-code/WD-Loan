            -
              code : |1-
               HourGlass(True)
               
               // AMOUNT OF REPAYMENTS
               EDT_AMOUNT_MONTHLYPAYMENTS1 = FinRepayVal(-EDT_LOAN_AMOUNT1, EDT_NBYEAR1*12, EDT_RATE1/12)
               
               // PAYMENT SCHEDULE
               EndPaymentSchedule(EDT_LOAN_AMOUNT1, EDT_AMOUNT_MONTHLYPAYMENTS1, EDT_RATE1/12, EDT_NBYEAR1*12, EDT_DEADLINE1)
               
               // TOTAL COST
               EDT_COST1 = -EndTotalInterest(EDT_LOAN_AMOUNT1, EDT_NBYEAR1*12, EDT_RATE1/12)
               
               // GRAND TOTAL
               EDT_TOTAL1 = EDT_COST1 + EDT_LOAN_AMOUNT1
               
               // CHARTS
               DrawLines(CHART_AMORTIZATION)
               DrawPie(EDT_LOAN_AMOUNT1,EDT_COST1, CHART_DISTRIBUTION)
               
               // next control to take focus
               ScreenFirst(TABLE_YEARLY_PAYMENT_SCHEDULE)
               
               HourGlass(False)
