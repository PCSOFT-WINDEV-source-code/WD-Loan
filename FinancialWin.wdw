        -
          name : BTN_CALC3
          identifier : 0x2717a4dc15b0feca
          internal_properties : 
          properties :
           x : 123
           y : 142
           width : 103
           height : 24
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              internal_properties : CAAAAAgAAACJa1zXZ0vwxtHad/hMRxa/sWve/EbXNwsZkAtGmMXp+Utm2HhVcnWx0okfJkwy23yh3MgExnoB1VX8/7BNYx/D6mJjljarw5W/8VlHS11hcRBao7rfDjWkrwesZ6zcbJ/+Wl40U+TQTALUdS4=
              code : |1-
               i is int
               nInterestAmount is currency
               nAmountCumulatedInterests is currency
               nCapitalAmount is currency
               nGainedAmount is currency
               dPaymentDate is date
               nNbYears is int 
               HourGlass(True)
               
               TableDeleteAll(TABLE_INVESTMENT)
               
               // date of first payment
               dPaymentDate = EDT_DEADLINE3
               nNbYears = EDT_NBYEAR3*12
               nCapitalAmount = EDIT_INITIAL_CAPITAL
               
               //Fill the table containing the payment schedule
               FOR i=1 TO nNbYears
               	
               	// The interests are paid at the end of each year
               	IF modulo(i,12)=0 THEN
               		//Calculate the interests for the year
               		nGainedAmount = -FinFutureVal(EDIT_INITIAL_CAPITAL, EDT_AMOUNT_MONTHLYPAYMENTS3, i, EDT_RATE3/12)
               		nInterestAmount = nGainedAmount-nCapitalAmount-EDT_AMOUNT_MONTHLYPAYMENTS3
               		nAmountCumulatedInterests += nInterestAmount
               		nCapitalAmount += nInterestAmount
               	END
               	
               	nCapitalAmount += EDT_AMOUNT_MONTHLYPAYMENTS3
               	
               	// the rounded values are required when exporting to Excel
               	TableAddLine(TABLE_INVESTMENT, i, dPaymentDate, Round(nCapitalAmount-EDT_AMOUNT_MONTHLYPAYMENTS3-nInterestAmount), Round(EDT_AMOUNT_MONTHLYPAYMENTS3), Round(nInterestAmount), Round(nAmountCumulatedInterests), Round(nCapitalAmount))
               	
               	nInterestAmount = 0
               	dPaymentDate..Month++
               END
               
               //Calculate the total gain
               EDIT_TOTALPAYMENT = EDIT_INITIAL_CAPITAL + (EDT_AMOUNT_MONTHLYPAYMENTS3 * EDT_NBYEAR3 *12 )
               EDIT_FINAL_CAPITAL = nGainedAmount
               EDIT_TOTALGAIN = EDIT_FINAL_CAPITAL - EDIT_TOTALPAYMENT
               
               
               // CHARTS
               DrawInvestmentLines(CHART_AMORTIZATION)
               DrawPie(EDIT_TOTALPAYMENT, (EDIT_FINAL_CAPITAL-EDIT_TOTALPAYMENT), CHART_DISTRIBUTION)
               
               // next control to take focus
               ScreenFirst(TABLE_INVESTMENT)
               
               HourGlass(False)
              type : 18
